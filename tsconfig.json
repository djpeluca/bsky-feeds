{
  "compilerOptions": {
    "lib": [
      "ESNext",
    ],
    "outDir": "dist",
    "module": "Node16",
    "target": "es2017",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false
  },
  "include": ["./src/**/*.ts"],
  "exclude": [
    "node_modules", "dist"
  ]
}